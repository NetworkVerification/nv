include "../utils.nv"

type attribute = option[int]

let trans e x =
    match x with
    | None -> None
    | Some n ->
      if n = 7 then None else Some (n + 1)

let merge n x1 x2 =
  mapbetter (fun n1 n2 -> if n1 < n2 then n1 else n2) x1 x2

let init n =
  match n with
  | 0n -> Some 0
  | _ -> None
