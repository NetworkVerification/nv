let interface edge =
  let any x = true
    in
    let nothingButBgp x =
      match x with
      | { connected = c; static = s; ospf = o; bgp = b; selected = sel } ->
        c = None && s = None && o = None
    in
    let hasOnlyBgp x =
      match x with
      | { connected = c; static = s; ospf = o; bgp = b; selected = sel } ->
        c = None && s = None && o = None && isSome b && sel = Some 3u2
    in
    match edge with
  | 1~14 -> Some hasOnlyBgp
  | 1~16 -> Some hasOnlyBgp
  | 1~18 -> Some hasOnlyBgp
  | 3~14 -> Some hasOnlyBgp
