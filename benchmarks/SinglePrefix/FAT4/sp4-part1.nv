let trans edge x =
  let o = transferOspf edge x.ospf in
  let b = transferBgp edge x in
  {connected=None; static=None; ospf=o; bgp=b; selected=None}

let assert_node node x =
  if (d = (70.0.19.0/24u6)) then
    match x.selected with
      | None -> false
      | _ -> true
  else true

